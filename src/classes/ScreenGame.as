package classes{	import flash.display.MovieClip;	import flash.events.MouseEvent;		public class ScreenGame extends MovieClip 	{		private var _holder_mc:MovieClip;				public function ScreenGame() 		{		}		public function init( holder_mc:MovieClip ):void		{			_holder_mc = holder_mc;						game_btn.buttonMode = true;			game_btn.mouseChildren = false;			game_btn.addEventListener( MouseEvent.MOUSE_UP, game_over );		}						public function setup_text( theXML:XML, lang:String ):void		{			game_btn.btn_txt.text = theXML..do_not_push_btn;		}						private function game_over( ev:MouseEvent ):void		{			_holder_mc.change_screen( ScreenConstants.SCREEN_END );			}						public function clean_up():void		{					}			}	}